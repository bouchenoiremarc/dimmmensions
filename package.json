{
  "name": "ios-dimensions",
  "version": "2.0.0",
  "description": "A collection of dimensions from iOS devices.",
  "author": "Marc Bouchenoire",
  "license": "MIT",
  "repository": "https://github.com/bouchenoiremarc/ios-dimensions",
  "keywords": [
    "ios",
    "dimensions",
    "layout",
    "guides"
  ],
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./dist/ios-dimensions.js",
  "umd:main": "./dist/ios-dimensions.umd.js",
  "module": "./dist/ios-dimensions.module.js",
  "exports": "./dist/ios-dimensions.modern.js",
  "esmodule": "./dist/ios-dimensions.modern.js",
  "esnext": "./dist/ios-dimensions.modern.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "microbundle --tsconfig tsconfig.build.json",
    "clean": "jest --clearCache && rimraf node_modules",
    "lint": "eslint '**/*.{ts,tsx}' --fix",
    "generate": "node -r esbuild-register scripts/generate.ts",
    "prepare": "rimraf dist && yarn build",
    "release": "yarn login && np",
    "test": "jest"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/jest": "^26.0.22",
    "@types/listr": "^0.14.2",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "app-exists": "^2.0.0",
    "command-exists": "^1.2.9",
    "esbuild-register": "^2.5.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "execa": "^5.0.0",
    "globby": "^11.0.3",
    "jest": "^26.6.3",
    "listr": "^0.14.3",
    "load-json-file": "^6.2.0",
    "microbundle": "^0.13.0",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "write-json-file": "^4.3.0"
  }
