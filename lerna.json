{
  "npmClient": "yarn",
  "registry": "https://registry.npmjs.org/",
  "publishConfig": {
    "access": "public"
  },
  "packages": [
    "packages/*"
  ],
  "useWorkspaces": true,
  "version": "2.0.1",
  "command": {
    "publish": {
      "allowBranch": "main",
      "conventionalCommits": true,
      "message": "%s"
    }
  }
}
