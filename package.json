{
  "name": "ios-dimensions",
  "version": "0.0.0",
  "description": "A collection of dimensions from iOS devices.",
  "author": "Marc Bouchenoire",
  "license": "MIT",
  "repository": "https://github.com/bouchenoiremarc/ios-dimensions",
  "keywords": [
    "ios",
    "dimensions",
    "layout",
    "guides"
  ],
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "source": "./src/index.ts",
  "main": "./dist/ios-dimension.js",
  "umd:main": "./dist/ios-dimension.umd.js",
  "module": "./dist/ios-dimension.module.js",
  "exports": "./dist/ios-dimension.modern.js",
  "esmodule": "./dist/ios-dimension.modern.js",
  "esnext": "./dist/ios-dimension.modern.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "microbundle --tsconfig tsconfig.build.json",
    "clean": "jest --clearCache && rimraf node_modules",
    "lint": "eslint '{src,tests}/**/*.{ts,tsx}' --fix",
    "generate": "ts-node-transpile-only scripts/generate.ts",
    "prepare": "rimraf dist && yarn build",
    "release": "yarn login && np",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/jest": "^26.0.22",
    "@types/listr": "^0.14.2",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "app-exists": "^1.1.0",
    "command-exists": "^1.2.9",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.0.0",
    "globby": "^11.0.2",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "listr": "^0.14.3",
    "load-json-file": "^6.2.0",
    "microbundle": "^0.13.0",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "write-json-file": "^4.3.0"
  }
}
